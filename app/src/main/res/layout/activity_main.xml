<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-136dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="376dp"

        android:layout_height="91dp"
        android:background="@drawable/rounded_rectangle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/map"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/map"
        app:layout_constraintVertical_bias="0.942" />

    <Button
        android:id="@+id/cameraBtn"
        android:layout_width="76dp"
        android:layout_height="61dp"
        android:layout_margin="5dp"
        android:background="@drawable/camera"
        android:padding="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.555"
        app:layout_editor_absoluteX="167dp"
        app:layout_editor_absoluteY="632dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/inventarioBtn"
        android:layout_width="76dp"
        android:layout_height="61dp"
        android:background="@drawable/clipboard"
        android:padding="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/cameraBtn"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.135"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="1.0"
        app:layout_editor_absoluteX="35dp"
        app:layout_editor_absoluteY="632dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/caminhoBtn"
        android:layout_width="76dp"
        android:layout_height="61dp"
        android:layout_margin="5dp"
        android:background="@drawable/caminhada"
        android:padding="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.876"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.533"
        app:layout_editor_absoluteX="298dp"
        app:layout_editor_absoluteY="632dp"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>