<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-136dp" />

    <Button
        android:id="@+id/cameraBtn"
        android:layout_width="76dp"
        android:layout_height="61dp"
        android:layout_margin="5dp"
        android:background="@drawable/camera"
        android:padding="8dp"

        app:layout_constraintBottom_toBottomOf="@+id/map"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.943"
        app:layout_editor_absoluteX="167dp"
        app:layout_editor_absoluteY="632dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/inventarioBtn"
        android:layout_width="76dp"
        android:layout_height="61dp"
        android:background="@drawable/clipboard"
        android:padding="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.131"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.943"
        app:layout_editor_absoluteX="35dp"
        app:layout_editor_absoluteY="632dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/caminhoBtn"
        android:layout_width="76dp"
        android:layout_height="61dp"
        android:layout_margin="5dp"
        android:background="@drawable/caminhada"
        android:padding="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.904"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.943"
        app:layout_editor_absoluteX="298dp"
        app:layout_editor_absoluteY="632dp"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>